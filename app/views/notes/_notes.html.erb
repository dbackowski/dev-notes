<% unless @notes.blank? %>
  <div class="row">
  <% @notes.each do |note| %>
    <div class="span-one-third">
      <div class="note">
        <h2><%= link_to note.title, note %></h2>
        <%= pretty_print note.description %>
        <div class="gradient"></div>
      </div>
    </div>
  <% end %>
  </div>

  <%= will_paginate @notes, :renderer => BootstrapPaginationLinkRenderer %>
<% else %>
  <%= info 'No notes found.' %>
<% end %>